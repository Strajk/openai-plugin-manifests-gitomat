{
  "openapi": "3.0.2",
  "info": {
    "title": "GetYourGuide Activity Service",
    "version": "0.1.0"
  },
  "paths": {
    "/v0/activities": {
      "get": {
        "tags": [
          "plugin"
        ],
        "summary": "Get Activities",
        "description": "Endpoint for finding GetYourGuide Activities",
        "operationId": "get_activities_v0_activities_get",
        "parameters": [
          {
            "description": "Name of the location the user is interested in",
            "required": false,
            "schema": {
              "title": "Location",
              "type": "string",
              "description": "Name of the location the user is interested in",
              "default": ""
            },
            "example": "Berlin",
            "name": "location",
            "in": "query"
          },
          {
            "description": "The date from which to look for activities",
            "required": false,
            "schema": {
              "title": "Start Date",
              "type": "string",
              "description": "The date from which to look for activities",
              "format": "date"
            },
            "example": "2023-08-11",
            "name": "start_date",
            "in": "query"
          },
          {
            "description": "The date up to which to look for activities",
            "required": false,
            "schema": {
              "title": "End Date",
              "type": "string",
              "description": "The date up to which to look for activities",
              "format": "date"
            },
            "example": "2023-08-21",
            "name": "end_date",
            "in": "query"
          },
          {
            "description": "Category name",
            "required": false,
            "schema": {
              "allOf": [
                {
                  "$ref": "#/components/schemas/Collections"
                }
              ],
              "description": "Category name"
            },
            "name": "category",
            "in": "query"
          },
          {
            "description": "Upper bound for price of an activity",
            "required": false,
            "schema": {
              "title": "Price Lte",
              "type": "number",
              "description": "Upper bound for price of an activity"
            },
            "name": "price_lte",
            "in": "query"
          },
          {
            "description": "Lower bound for price of an activity",
            "required": false,
            "schema": {
              "title": "Price Gte",
              "type": "number",
              "description": "Lower bound for price of an activity",
              "default": 0
            },
            "name": "price_gte",
            "in": "query"
          },
          {
            "description": "Lower bound for time of day when activity is available, formated as HH:MM",
            "required": false,
            "schema": {
              "title": "Start Time",
              "type": "string",
              "description": "Lower bound for time of day when activity is available, formated as HH:MM",
              "default": "00:01"
            },
            "example": "07:25",
            "name": "start_time",
            "in": "query"
          },
          {
            "description": "Upper bound for time of day when activity is available, formatted as HH:MM",
            "required": false,
            "schema": {
              "title": "End Time",
              "type": "string",
              "description": "Upper bound for time of day when activity is available, formatted as HH:MM",
              "default": "23:59"
            },
            "example": "21:53",
            "name": "end_time",
            "in": "query"
          },
          {
            "description": "Short user query in English",
            "required": false,
            "schema": {
              "title": "Query",
              "type": "string",
              "description": "Short user query in English",
              "default": ""
            },
            "name": "query",
            "in": "query"
          },
          {
            "description": "How many results should be skipped",
            "required": false,
            "schema": {
              "title": "Skip",
              "type": "integer",
              "description": "How many results should be skipped",
              "default": 0
            },
            "name": "skip",
            "in": "query"
          },
          {
            "description": "The amount of activities returned to the user",
            "required": false,
            "schema": {
              "title": "Amount Of Activities",
              "type": "integer",
              "description": "The amount of activities returned to the user",
              "default": 4
            },
            "name": "amount_of_activities",
            "in": "query"
          },
          {
            "description": "Currency of the User (default USD)",
            "required": false,
            "schema": {
              "title": "Currency",
              "type": "string",
              "description": "Currency of the User (default USD)",
              "default": "USD"
            },
            "name": "currency",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/QueryResponse"
                }
              }
            }
          },
          "404": {
            "description": "No activities found"
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
