{
  "openapi": "3.0.2",
  "info": {
    "title": "FastAPI",
    "version": "0.1.0"
  },
  "paths": {
    "/.well-known/ai-plugin.json": {
      "get": {
        "summary": "Serve Manifest",
        "operationId": "serve_manifest__well_known_ai_plugin_json_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
    "/search-keywords": {
      "get": {
        "summary": "Search Keywords",
        "operationId": "search_keywords_search_keywords_get",
        "parameters": [
          {
            "description": "keywords comma separated",
            "required": true,
            "schema": {
              "title": "Keywords",
              "type": "string",
              "description": "keywords comma separated"
            },
            "example": "pdf,reader",
            "name": "keywords",
            "in": "query"
          },
          {
            "description": "date in format YYYY-MM-DD",
            "required": true,
            "schema": {
              "title": "Date From",
              "pattern": "^\\d{4}-\\d{2}-\\d{2}$",
              "type": "string",
              "description": "date in format YYYY-MM-DD"
            },
            "example": "2023-08-09",
            "name": "date_from",
            "in": "query"
          },
          {
            "description": "date in format YYYY-MM-DD",
            "required": true,
            "schema": {
              "title": "Date To",
              "pattern": "^\\d{4}-\\d{2}-\\d{2}$",
              "type": "string",
              "description": "date in format YYYY-MM-DD"
            },
            "example": "2023-08-24",
            "name": "date_to",
            "in": "query"
          },
          {
            "required": false,
            "schema": {
              "title": "Country Code",
              "type": "string",
              "default": "US"
            },
            "name": "country_code",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SearchKeywordsResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "HTTPValidationError": {
        "title": "HTTPValidationError",
        "type": "object",
        "properties": {
          "detail": {
            "title": "Detail",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            }
          }
        }
      },
      "Keyword": {
        "title": "Keyword",
        "required": [
          "keyword",
          "date"
        ],
        "type": "object",
        "properties": {
          "keyword": {
            "title": "Keyword",
            "type": "string"
          },
          "date": {
            "title": "Date",
            "type": "string"
          },
          "country_code": {
            "title": "Country Code",
            "type": "string"
          },
          "popularity_score": {
            "title": "Popularity Score",
            "type": "integer"
          },
          "max_daily_impressions": {
            "title": "Max Daily Impressions",
            "type": "integer"
          }
        }
      },
      "SearchKeywordsResponse": {
        "title": "SearchKeywordsResponse",
        "required": [
          "data"
        ],
        "type": "object",
        "properties": {
          "data": {
            "title": "Data",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Keyword"
            }
          },
          "not_found_msg": {
            "title": "Not Found Msg",
            "type": "string"
          }
        }
      },
      "ValidationError": {
        "title": "ValidationError",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "type": "object",
        "properties": {
          "loc": {
            "title": "Location",
            "type": "array",
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            }
          },
          "msg": {
            "title": "Message",
            "type": "string"
          },
          "type": {
            "title": "Error Type",
            "type": "string"
          }
        }
      }
    }
  }
}