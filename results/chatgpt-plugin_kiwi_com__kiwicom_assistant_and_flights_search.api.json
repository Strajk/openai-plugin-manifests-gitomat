            "description": "IATA code of the departure location. Accepting multiple values separated by comma.",
            "required": true,
            "schema": {
              "type": "string",
              "description": "IATA code of the departure location. Accepting multiple values separated by comma."
            },
            "name": "fly_from",
            "in": "query"
          },
