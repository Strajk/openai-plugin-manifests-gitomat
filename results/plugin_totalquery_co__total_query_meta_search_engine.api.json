{
  "openapi": "3.0.0",
  "paths": {
    "/search": {
      "get": {
        "operationId": "search",
        "summary": "Search the internet for anything.",
        "description": "Search the internet for news articles, blog posts, scientific papers, videos, images, Q&A sites and much more.",
