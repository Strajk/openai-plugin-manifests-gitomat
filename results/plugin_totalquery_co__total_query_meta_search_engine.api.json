          {
            "name": "q",
            "required": true,
            "in": "query",
            "description": "The search query.",
            "example": "Why is vim still the best editor as of 2023",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "engines",
            "required": false,
            "in": "query",
            "description": "A comma-separated list of search engines to use. Mandatory.",
            "schema": {
              "format": "comma-separated",
              "type": "array",
              "items": {
